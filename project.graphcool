type Post implements Node {
  description: String!
  createdAt: DateTime!
  id: ID! @isUnique
  updatedAt: DateTime!
  title: String!
  url: String!
}

type User implements Node {
  createdAt: DateTime!
  id: ID! @isUnique
  name: String!
  email: String!
  emailSubscription: Boolean!
  updatedAt: DateTime!
}

type File implements Node {
  contentType: String!
  createdAt: DateTime!
  id: ID! @isUnique
  name: String!
  secret: String! @isUnique
  size: Int!
  updatedAt: DateTime!
  url: String! @isUnique
}